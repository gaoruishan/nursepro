<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_86"
    android:orientation="vertical"
    android:background="@color/white">
    <LinearLayout
        android:id="@+id/ll_group"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_85"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/img_telstatu"
            android:layout_width="@dimen/dp_40"
            android:layout_height="@dimen/dp_40"
            android:src="@drawable/nurlink_icon_call_list"
            android:gravity="center"
            android:layout_marginLeft="@dimen/sp_20"></ImageView>
        <TextView
            android:id="@+id/tv_group_name"
            android:layout_width="0dp"
            android:layout_height="@dimen/dp_40"
            android:layout_weight="1"
            android:text="护士组"
            android:textStyle="bold"
            android:textSize="@dimen/dp_28"
            android:gravity="center"
            android:textColor="@color/black"></TextView>
        <TextView
            android:id="@+id/tv_tel_call"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_25"
            android:layout_marginRight="@dimen/dp_10"
            android:gravity="center"
            android:background="@drawable/dhcc_setting_arrow"
            android:textColor="@color/white"></TextView>

    </LinearLayout>
   <View
       android:layout_width="match_parent"
       android:layout_height="@dimen/dp_1"
       android:background="#FFF4F4F4"></View>

</LinearLayout>