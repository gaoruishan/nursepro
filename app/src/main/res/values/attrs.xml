<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SelectTextView">
        <attr name="unSelectTextColor" format="color" />
        <attr name="selectedTextColor" format="color" />
        <attr name="selectedIconWidth" format="dimension" />
        <attr name="selectedIconHeight" format="dimension" />
        <attr name="selectedIcon" format="reference" />
        <attr name="selectedBg" format="reference" />
        <attr name="unSelectedBg" format="reference" />
    </declare-styleable>

    <declare-styleable name="SelectorImageView">
        <attr name="selectedImage" format="reference" />
        <attr name="unselectedImage" format="reference" />
        <attr name="isCheck" format="boolean" />
    </declare-styleable>
    <declare-styleable name="NursepassBlood">
        <attr name="nursehint" format="string" />
        <attr name="passhint" format="string" />
    </declare-styleable>
</resources>